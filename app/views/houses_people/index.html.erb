<p id="notice"><%= notice %></p>

<h1>Listing Houses People</h1>

<table class="table">
  <thead>
    <tr>
      <th>House</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @houses_people.each do |houses_person| %>
      <tr>
        <td><%= houses_person.house_id %></td>
        <td><%= houses_person.person_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<% if get_permission %>
	<%= link_to 'New Houses people', new_houses_person_path %>
<% end %>
